modifier onlyTwoPlayer(){
        require (participantRegistered <= 2 , "Exactly two players can participate! ");
        _;
    }
    modifier notAlreadyRegistered()
    {
        require ( !(participantNumber[msg.sender] > 0) , "You are already registered in the quiz.");
        _;
    }
